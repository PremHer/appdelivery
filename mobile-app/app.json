{
  "expo": {
    "name": "Sajino Express",
    "slug": "delivery-app-prem",
    "scheme": "deliveryapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2D2D2D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prem.deliveryapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesitamos tu ubicaci칩n para mostrarte restaurantes cercanos y calcular tiempos de entrega.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Necesitamos tu ubicaci칩n para tracking de pedidos en tiempo real.",
        "NSCameraUsageDescription": "Necesitamos acceso a la c치mara para que puedas subir fotos de perfil.",
        "NSPhotoLibraryUsageDescription": "Necesitamos acceso a tus fotos para que puedas subir una foto de perfil."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpg",
        "backgroundColor": "#2D2D2D"
      },
      "package": "com.prem.deliveryapp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir acceso a ubicaci칩n para tracking de pedidos."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir acceso a fotos para subir imagen de perfil."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.319484744982-nbef4414ui66r9on2l85hchfmbgpjroi"
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "56e695f6-8b8b-4532-93df-0a39bb80f28f"
      }
    },
    "owner": "hernandezpremh",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/56e695f6-8b8b-4532-93df-0a39bb80f28f"
    }
  }
}
